
<div class="row">
<div class="col s8">
  <h4>New Payroll:</h4>
  <%= simple_form_for(:payroll) do |f| %>
    <%= f.error_notification %>
    <div class="row">
      <div class="form-inputs">
        <%= f.input :date_range %>
      </div>
    </div>
    <div class="row">
      <div class="form-actions col s8">
        <%= f.button :submit %>
      </div>
    </div>
  <% end %>
</div>

</div>

